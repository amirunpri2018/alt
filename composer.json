{
  "name": "ankitpokhrel/alt",
  "description": "Automatic alternative (alt) text for images using object detection with pre-trained model.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ankit Pokhrel",
      "email": "hello@ankit.pl"
    }
  ],
  "require": {
    "php": "^7.1.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.9",
    "phpunit/phpunit": "6.5.14"
  },
  "autoload": {
    "psr-4": {
      "Alt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Alt\\Test\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit",
    "cs-fixer": "php-cs-fixer fix src/ --rules=@PSR2,not_operator_with_space,single_quote,-single_import_per_statement"
  }
}
